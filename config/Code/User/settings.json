{
    "emmet.includeLanguages": {
        "rust": "html",
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "sql-formatter.uppercase": true,
    "cmake.configureOnOpen": false,
    "cSpell.enabled": true,
    "cSpell.enableFiletypes": [
        "plaintext"
    ],
    "cSpell.userWords": [
        "Addrs",
        "onclick",
        "clippy",
        "dockercompose",
        "jquery",
        "oneshot",
        "paypal",
        "PYTHONPATH",
        "tailwindcss",
        "venv",
        "virtualenv"
    ],
    "path-intellisense.absolutePathToWorkspace": false,
    "dev.containers.dockerComposePath": "podman-compose",
    "dev.containers.dockerPath": "podman",
    "docker.composeCommand": "podman-compose",
    "docker.dockerPath": "podman",
    "editor.fontFamily": "'Hack Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
    "editor.rulers": [
        90,
        120
    ],
    "editor.tokenColorCustomizations": {
        "comments": "#606060"
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "afterDelay",
    "python.analysis.extraPaths": [
        "${workspaceFolder}/tst",
        "${workspaceFolder}/src",
        "${workspaceFolder}"
    ],
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}/tst",
        "${workspaceFolder}/src",
        "${workspaceFolder}"
    ],
    "python.envFile": "${workspaceFolder}/.venv",
    "python.venvFolders": [
        "${workspaceFolder}/.venv",
        "${workspaceFolder}/.virtualenv",
        "${workspaceFolder}/.env"
    ],
    "redhat.telemetry.enabled": false,
    "security.workspace.trust ui.untrustedFiles": "open",
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}/tst:${workspaceFolder}/src:${env:PYTHONPATH}"
    },
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path-linux": "/home/baptiste/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
        "vscode-kubernetes.minikube-path-linux": "/home/baptiste/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.minikube-show-information-expiration": "2024-12-30T12:40:28.366Z"
    },
    "window.menuBarVisibility": "toggle",
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#35199b93"
    },
    "cmake.showOptionsMovedNotification": false,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "tailwindCSS.emmetCompletions": true,
    "tailwindCSS.includeLanguages": {
        "rust": "html"
    },
    "[tailwindCSS]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "errorLens.lintFilePaths": {
        "tailwindCSS": [
            "**/*.tailwind.config.{cjs,js,json,yaml,yml}",
            "**/*package.json",
            "**/*.css"
        ],
        "eslint": [
            "**/*.eslintrc.{js,cjs,yaml,yml,json}",
            "**/*package.json"
        ],
        "Stylelint": [
            "**/*.stylelintrc",
            "**/*.stylelintrc.{cjs,js,json,yaml,yml}",
            "**/*stylelint.config.{cjs,js}",
            "**/*package.json"
        ]
    },
    "files.associations": {
        "*.css": "tailwindcss",
        "*.twig": "html"
    },
    "editor.quickSuggestions": {
        "strings": "on"
    },
    // "rust-analyzer.checkOnSave.command": "clippy",
    "[css]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "[dockercompose]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.unicodeHighlight.invisibleCharacters": false,
    "dev.containers.dockerCredentialHelper": false,
    "docker.commands.attach": "${containerCommand} exec -it ${containerId} ${shellCommand}",
    "docker.contexts.showInStatusBar": true,
    "vim.handleKeys": {
        "<C-d>": false
    },
    "vim.useCtrlKeys": false,
    "docker.environment": {
        "DOCKER_HOST": "unix:///run/user/1000/podman/podman.sock"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "powermode.enabled": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.startupEditor": "none",
    "liveServer.settings.donotShowInfoMsg": true,
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "github.copilot.editor.enableAutoCompletions": true,
    "[javascript]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "git.openRepositoryInParentFolders": "always"
}