{
    "cmake.configureOnOpen": false,
    "cSpell.enabled": true,
    "cSpell.enableFiletypes": [
        "plaintext"
    ],
    "cSpell.userWords": [
        "PYTHONPATH",
        "venv",
        "virtualenv"
    ],
    "dev.containers.dockerComposePath": "podman-compose",
    "dev.containers.dockerPath": "podman",
    "docker.composeCommand": "podman-compose",
    "docker.dockerPath": "podman",
    "editor.fontFamily": "'Hack Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
    "editor.rulers": [
        80,
        120
    ],
    "editor.tokenColorCustomizations": {
        "comments": "#606060"
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "afterDelay",
    "python.analysis.extraPaths": [
        "${workspaceFolder}/tst",
        "${workspaceFolder}/src",
        "${workspaceFolder}"
    ],
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}/tst",
        "${workspaceFolder}/src",
        "${workspaceFolder}"
    ],
    "python.envFile": "${workspaceFolder}/.venv",
    "python.venvFolders": [
        "${workspaceFolder}/.venv",
        "${workspaceFolder}/.virtualenv",
        "${workspaceFolder}/.env"
    ],
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}/tst:${workspaceFolder}/src:${env:PYTHONPATH}"
    },
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path-linux": "/home/baptiste/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
        "vscode-kubernetes.minikube-path-linux": "/home/baptiste/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube"
    },
    "window.menuBarVisibility": "toggle",
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#35199b93"
    }
}
